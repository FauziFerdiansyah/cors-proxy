{
  "version": 2,
  "builds": [
    {
      "src": "server.js", // Ganti dengan nama file server Anda
      "use": "@vercel/node" // Gunakan jika Anda memiliki aplikasi Node.js
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server.js" // Ganti dengan nama file server Anda
    }
  ]
}
